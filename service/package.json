{
  "name": "gateway-gauntlet-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "NODE_ENV=production node dist/index.js",
    "build": "tsc -b",
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node' src/index.ts",
    "test": "jest --watch --collect-coverage",
    "test:ci": "CI=true jest --env=jsdom --verbose --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "ci": "yarn lint && yarn test:ci"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ]
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/express-serve-static-core": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^8.0.29",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "typedi": "^0.8.0",
    "typeorm": "0.2.22",
    "typeorm-typedi-extensions": "^0.2.3"
  }
}
